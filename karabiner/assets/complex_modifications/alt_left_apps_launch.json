{
  "title": "Launch my preffered apps",
  "rules": [
    {
      "description": "Right ALT + 1 iTerm / + 2 Chrome / + 3 Slack / + 4 Teams / + 5 Studio",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "r"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/iTerm.app"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Google\\ Chrome.app"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Slack.app"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "4",
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Microsoft\\ Teams.app"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open ~/Applications/Studio\\ 3T.app"
            }
          ]
        }
      ]
    }
  ]
}
